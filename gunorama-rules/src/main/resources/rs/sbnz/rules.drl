package rs.sbnz;

import  sbnz.integracija.example.facts.Item;


rule "Classify Item - Low Range"
    when
        $i: Item( cost <= 300, category == Item.Category.NA )
    then
    	System.out.println("USAO U PRAVILO ITEM U NASEM PROJEKTU");
        $i.setCategory(Item.Category.LOW_RANGE);
        update($i);
end

rule "Classify Item - High Range"
    when
        $i: Item( cost > 300, category == Item.Category.NA )
    then
    	System.out.println("USAO U PRAVILO HIGH ITEM U NASEM PROJEKTU");
        $i.setCategory(Item.Category.HIGH_RANGE);
        update($i);
end


